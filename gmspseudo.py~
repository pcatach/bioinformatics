def gms(k, t, Dna):
    bestMotifs = [ string[:k] for string in Dna ]
    for motif in [ string[:k] for string in Dna ]:
        motifs = [motif]
        for i in range(1, t):
            profile = formProfile(motifs, k)
            motifs.append( mostProbable(profile, Dna[i]) )
        if score(motifs) < score(bestMotifs):
            bestMotifs = motifs
    return bestMotifs

k, t, Dna = read('data.txt')
