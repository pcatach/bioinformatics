'''LEADERBOARDCYCLOPEPTIDESEQUENCING.

Input: Integer N and a collection of integers Spectrum.

Output: LeaderPeptide after running LEADERBOARDCYCLOPEPTIDESEQUENCING(Spectrum, N).'''

def main(spec,n):
    leaderboard=[[]]
    leaderpeptide=[]
    while len(leaderboard)>0:
        leaderboard=expand(leaderboard)
        for peptide in leaderboard:
            if sum(peptide)==spec[len(spec)-1]:
                if score(peptide,spectrum)>score(leaderpeptide,spectrum):
                    leaderpeptide=peptide
            elif sum(peptide)>spec[len(spec)-1]:
                leaderboard.remove(peptide)
        leaderboard=cut(leaderboard,spec,n)
    print(leaderpeptide)


def score(peptide,refspectrum):
    #print(peptide, end=' ')
    subpeptides=[]
    for i in range(1,len(peptide)+1):
        for j in range(len(peptide)):
            try:
                if j+i<=len(peptide):
                    l=peptide[j:j+i]
                    subpeptides.append(l)
            except:
                pass
    
    spectrum=[]
    for i in subpeptides:
        spectrum.append(sum(i))
    #print(spectrum)

    s=0
    for i in spectrum:
        if i in refspectrum:
            s+=1
    return s
    
    

def cut(leaderboard,spec,n):
    scorelist=[]
    ans=leaderboard[:]
    for i in leaderborad:
        scorelist.append(score(i,spec))
    cut=[]
    for k in range(len(scorelist)):
        if scorelist[k]<n:
            cut.append(k)
    for j in range(len(cut)):
        ans.remove(leaderboard[cut[j]])
    return ans



def expand(leaderboard):
    table={}
    file=open('integer_mass_table.txt')
    for line in file:
        table[line[0]]=int(line[2:])
    file.close()

    items=[j for j in table.values()]
    extended=[]
    for i in leaderboard:
        for k in items:
            r=i[:]
            r.append(k)
            extended.append(r)
    return extended




N=input()
Spectrum=input()
main(Spectrum,N)
