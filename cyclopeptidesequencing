def main():

    table={}
    table['0']=0
    file=open('integer_mass_table.txt')
    for line in file:
        table[line[0]]=int(line[2:])
    file.close()
    
    lista=[0]
    while len(lista)>0:
        lista=expand(lista, table)
        for peptide in lista:
            if cyclospectrum(peptide)==spectrum:
                print(peptide)
                remove peptide from lista
            else if peptide is not consistent with spectrum:
                remove peptide from lista
    







def expand(lista,table):
    extended=[]
    for i in table.items():
        for elem in lista:
            extended.append(elem+i)
    return extended
    
main()
